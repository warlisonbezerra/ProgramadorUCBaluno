#include<stdio.h>
int main(){
int A,B;
int SOMA;

printf("Digite a letra A:\n");
scanf("%d", &A);
printf("Digite a letra B:\n");
scanf("%d", &B);
SOMA= A + B;
printf("%d + %d = \n%d e", A, B, SOMA);

}
